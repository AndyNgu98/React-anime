div>\n          </div>\n        </div>\n      </section>\n\n      <section className='section preview'>\n        <div className='container'>\n          <div className='columns'>\n          {\n            characters.map((character) => (\n              <div className='column preview__character-img' key={character.mal_id}>\n                <img className='mb-3' src={character.image_url} alt={character.name}/>\n                <p><a className='preview__link' href={character.url} rel=\"noreferrer\">{character.name}</a></p>\n              </div>\n            ))\n          }\n          </div>\n        </div>\n      </section>  \n    </>\n  )\n}\n\n            \n\nexport default AnimePreview\n","/Users/andy/anime-project/src/Components/Spinner/Spinner.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":4,"column":8,"nodeType":"108","messageId":"109","endLine":4,"endColumn":16},{"ruleId":"106","severity":1,"message":"110","line":7,"column":10,"nodeType":"108","messageId":"109","endLine":7,"endColumn":18},{"ruleId":"106","severity":1,"message":"111","line":8,"column":10,"nodeType":"108","messageId":"109","endLine":8,"endColumn":16},{"ruleId":"106","severity":1,"message":"112","line":14,"column":10,"nodeType":"108","messageId":"109","endLine":14,"endColumn":17},{"ruleId":"106","severity":1,"message":"113","line":4,"column":9,"nodeType":"108","messageId":"109","endLine":4,"endColumn":20},{"ruleId":"114","severity":1,"message":"115","line":13,"column":5,"nodeType":"116","endLine":13,"endColumn":7,"suggestions":"117"},{"ruleId":"102","replacedBy":"118"},{"ruleId":"104","replacedBy":"119"},{"ruleId":"106","severity":1,"message":"120","line":5,"column":10,"nodeType":"108","messageId":"109","endLine":5,"endColumn":21},{"ruleId":"114","severity":1,"message":"121","line":27,"column":5,"nodeType":"116","endLine":27,"endColumn":7,"suggestions":"122"},{"ruleId":"106","severity":1,"message":"123","line":70,"col